#include "common.h"

struct	p_TL
{
	float2 	Tex0	: TEXCOORD0;
	float4	Color	: COLOR0;
};

//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
float4 main( p_TL I ) : COLOR
{
	return tex2D( s_base, I.Tex0 ) * float4( ( L_sun_color * 4 ), 1 );
}
